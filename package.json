{
  "name": "suffragium",
  "version": "1.0.0",
  "description": "Create polls quickly and watch the results change in real time!",
  "main": "app.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node server/app",
    "postinstall": "bower install",
    "dev": "nodemon server/app",
    "dev:debug": "DEBUG=*,-nodemon*,-send* npm run dev",
    "build:scss": "node-sass client/scss/styles.scss  client/css/styles.css",
    "build:scss:watch": "node-sass client/scss/styles.scss  client/css/styles.css --watch"
  },
  "keywords": [
    "poll",
    "survey"
  ],
  "author": "jalbertsr",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.4",
    "mongoose": "^4.11.7"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0"
  }
}
